defmodule <%= mod_name %>.ProxyService do
  @moduledoc false
  use GRPC.Server, service: <%= name %>

<%= Enum.map methods, fn(method) -> %>
  def <%= method %>(message, stream), 
    do: <%= handler %>.dispatch(%{entity_type: "<%= entity_type %>", persistence_id: "<%= persistence_id %>", message: message, stream: stream})
<% end %>

end